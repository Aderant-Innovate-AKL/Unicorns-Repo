{
  "name": "hackathon-ui-template",
  "description": "A hackathon-ready template for React apps with TypeScript, Vite, and Material UI - no backend required!",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "format": "prettier --write .",
    "lint:fix": "eslint --fix .",
    "preview": "vite preview",
    "test": "vitest",
    "test:silent": "vitest --silent",
    "test:watch": "vitest --silent --watch",
    "test:coverage": "vitest --coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "prepare": "husky"
  },
  "dependencies": {
    "@aderant/stridyn-askmaddi": "^2.21.0",
    "@aderant/stridyn-components": "^2.21.0",
    "@aderant/stridyn-foundation": "^2.21.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^6.5.0",
    "@mui/material": "^6.5.0",
    "@mui/system": "^6.5.0",
    "@mui/x-data-grid-pro": "^7.29.9",
    "@mui/x-license": "^7.29.1",
    "@tanstack/react-query": "^5.62.11",
    "axios": "^1.9.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.9.4",
    "react-router-dom": "^7.9.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@playwright/test": "^1.56.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.18.11",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "3.2.3",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.2",
    "playwright-ctrf-json-reporter": "^0.0.18",
    "prettier": "^3.6.2",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^6.4.1",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.0.0",
  "lint-staged": {
    "**/*": [
      "prettier --write --ignore-unknown",
      "eslint --fix"
    ]
  }
}
